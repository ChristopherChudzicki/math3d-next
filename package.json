{
  "name": "math3d-next",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.1.1",
  "devDependencies": {
    "@math3d/eslint-config": "workspace:*",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "echo": "echo $CAT",
    "lint": "turbo run lint",
    "test": "dotenv -- turbo run test",
    "test-e2e": "dotenv -- yarn workspace app-tests-e2e test-e2e",
    "typecheck": "turbo run typecheck",
    "start": "dotenv -- yarn workspace app start",
    "build": "dotenv -- yarn workspace app build",
    "preview": "dotenv -- yarn workspace app preview"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.27.1",
    "prettier": "^3.1.0"
  },
  "msw": {
    "workerDirectory": "packages/app/public"
  }
}
